{
	"version": "2.0",
	"extensionBundle": {
		"id": "Microsoft.Azure.Functions.ExtensionBundle",
		"version": "[2.*, 3.0.0)"
	},
	"customHandler": {	    
		"description": {
			"defaultExecutablePath": "go-azure-function"
		},
		"enableForwardingHttpRequest": true
	},
	"extensions": {
		"cosmosDB": {
				"connectionMode": "Gateway",
				"protocol": "Https",
				"leaseOptions": {
						"leasePrefix": "prefix1"
				}
		}
  }
}