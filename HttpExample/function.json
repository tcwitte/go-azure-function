{
	"bindings": [
	{
		"type": "httpTrigger",
		"direction": "in",
		"methods": ["GET"],
		"name": "req",
		"authLevel": "anonymous"
	},
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "$return",
      "type": "CosmosDBOutput",
      "direction": "out",
      "databaseName": "mydatabase",
			"collectionName": "mycollection",
      "connectionStringSetting": "CosmosDBConnectionString"      
    }
]
}